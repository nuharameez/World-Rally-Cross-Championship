<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.progcw.RaceTable">
   <children>
      <Button layoutX="526.0" layoutY="360.0" mnemonicParsing="false" onAction="#onGoBackButtonClick" text="Go Back" />
      <TableView fx:id="raceTable" layoutX="54.0" layoutY="79.0" prefHeight="265.0" prefWidth="494.0">
        <columns>
          <TableColumn fx:id="date" prefWidth="82.4000244140625" text="Date" />
          <TableColumn fx:id="raceLocation" prefWidth="92.79998779296875" text="Location" />
            <TableColumn fx:id="drivers" minWidth="0.0" prefWidth="318.4000244140625" text="Drivers" />
        </columns>
      </TableView>
      <Button layoutX="375.0" layoutY="360.0" mnemonicParsing="false" onAction="#raceTableShow" text="Click here to view table" />
      <Label alignment="CENTER" layoutX="217.0" layoutY="24.0" text="Race Table" textFill="#21288e">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="35.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
